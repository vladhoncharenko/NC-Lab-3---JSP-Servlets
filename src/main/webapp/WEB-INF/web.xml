<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>MainServlet</servlet-name>
        <servlet-class>servlets.MainServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SqlExecutorServlet</servlet-name>
        <servlet-class>servlets.SqlExecutorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addDeptntServlet</servlet-name>
        <servlet-class>servlets.AddDeptntServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addEmplServlet</servlet-name>
        <servlet-class>servlets.AddEmplServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>indexServlet</servlet-name>
        <servlet-class>servlets.IndexServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginRegisterServlet</servlet-name>
        <servlet-class>servlets.LoginRegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeptntBrowserServlet</servlet-name>
        <servlet-class>servlets.DeptntBrowserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmplBrowserServlet</servlet-name>
        <servlet-class>servlets.EmplBrowserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteDeptnt</servlet-name>
        <servlet-class>servlets.DeleteDeptnt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteEmpl</servlet-name>
        <servlet-class>servlets.DeleteEmpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeptntEditServlet</servlet-name>
        <servlet-class>servlets.DeptntEditServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginRegisterServlet</servlet-name>
        <url-pattern>login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MainServlet</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SqlExecutorServlet</servlet-name>
        <url-pattern>/execute</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addDeptntServlet</servlet-name>
        <url-pattern>/departmentadd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addEmplServlet</servlet-name>
        <url-pattern>/employeeadd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    <servlet-name>indexServlet</servlet-name>
    <url-pattern>index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeptntBrowserServlet</servlet-name>
        <url-pattern>/departments</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmplBrowserServlet</servlet-name>
        <url-pattern>/employees</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteDeptnt</servlet-name>
        <url-pattern>/deletedepartment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteEmpl</servlet-name>
        <url-pattern>/deleteemployee</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeptntEditServlet</servlet-name>
        <url-pattern>/editdepartment</url-pattern>
    </servlet-mapping>
</web-app>
